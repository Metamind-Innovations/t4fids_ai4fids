{
    "paths": {
        "data": "data",
        "model_save": "models",
        "results_save": "results"
    },
    "server": {
        "address": "127.0.0.1",
        "port": "8080"
    },
    "tr_params": {
        "learning_rate": 0.1,
        "batch_size": 64,
        "local_epochs": 5
    },
    "data_kw": {
        "features_to_drop": [
            "flow_id",
            "ip_src",
            "ip_dst",
            "udp_sport",
            "udp_dport"
        ],
        "label_keyword": "label"
    },
    "misc": {
        "override_params": false,
        "resample_flag": false
    },
    "opt": {
        "optimizer": "RMSprop",
        "params": {
            "rho": 0.9,
            "momentum": 0.0
        }
    }
}